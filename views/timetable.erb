<% @timetables.each do |timetable| %>
  <div class="overflow-x-auto">
    <table class="table w-full">
      <!-- head -->
      <thead>
        <tr>
          <th>From</th>
          <th>To</th>
          <th>Dep</th>
          <th>Arr</th>
        </tr>
      </thead>
      <tbody>
      <% timetable.each do |row| %>
        <!-- row 1 -->
        <tr>
        <% row[0..3].each do |cell| %>
          <td><%= cell %></td>
        <% end %>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
<% end %>
